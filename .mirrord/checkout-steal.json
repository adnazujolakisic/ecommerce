{
   "target":{
      "path":{
         "deployment":"checkout"
      },
      "namespace":"metalmart"
   },
   "feature":{
      "network":{
         "incoming":"steal",
         "outgoing":{
            "tcp":true,
            "udp":true,
            "filter":{
               "local":[
                  "localhost:8082",
                  "127.0.0.1:8082"
               ]
            }
         },
         "dns":true
      },
      "fs":"local",
      "env":{
         "override":{
            "INVENTORY_SERVICE_URL":"http://localhost:8082"
         }
      }
   },
   "agent":{
      "namespace":"metalmart"
   },
   "operator":true
}