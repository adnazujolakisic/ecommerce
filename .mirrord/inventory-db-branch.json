{
  "target": {
    "path": {
      "deployment": "inventory"
    },
    "namespace": "metalmart"
  },
  "feature": {
    "network": {
      "incoming": "mirror",
      "outgoing": {
        "tcp": true,
        "udp": true
      },
      "dns": true
    },
    "fs": "local",
    "env": true,
    "db_branches": [
      {
        "type": "pg",
        "version": "16",
        "name": "inventory",
        "ttl_secs": 300,
        "creation_timeout_secs": 60,
        "connection": {
          "url": {
            "type": "env",
            "variable": "DATABASE_URL"
          }
        },
        "copy": {
          "mode": "all"
        }
      }
    ]
  },
  "agent": {
    "namespace": "metalmart"
  },
  "operator": true
}
