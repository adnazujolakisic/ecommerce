{
   "target":{
      "path":{
         "deployment":"inventory"
      },
      "namespace":"metalmart"
   },
   "feature":{
      "network":{
         "incoming":{
            "mode":"steal",
            "http_filter":{
               "header_filter":"X-PG-Tenant: Adna"
            }
         },
         "outgoing":{
            "tcp":true,
            "udp":true
         },
         "dns":true
      },
      "fs":"local",
      "env":{
         "override":{
            "MIRRORD_DB_BRANCH":"true"
         }
      },
      "db_branches":[
         {
            "type":"pg",
            "version":"16",
            "name":"inventory",
            "ttl_secs":300,
            "creation_timeout_secs":180,
            "connection":{
               "url":{
                  "type":"env",
                  "variable":"DATABASE_URL"
               }
            },
            "copy":{
               "mode":"schema",
               "tables":{
                  "inventory":{
                     "filter":"product_id IN ('1','2','3')"
                  }
               }
            }
         }
      ]
   },
   "agent":{
      "namespace":"metalmart"
   },
   "operator":true,
   "experimental":{
      "disable_reuseaddr":true
   }
}